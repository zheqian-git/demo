<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CharitySchoolMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.domain.CharitySchool">
        <id column="id" property="id" />
        <result column="image_url" property="imageUrl" />
        <result column="class_name" property="className" />
        <result column="class_date" property="classDate" />
        <result column="class_time" property="classTime" />
        <result column="class_location" property="classLocation" />
        <result column="class_detail" property="classDetail" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="status" property="status" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modify" property="gmtModify" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, image_url, class_name, class_date, class_time, class_location, class_detail, start_time, end_time, status, gmt_create, gmt_modify
    </sql>

</mapper>
